                        <Password>
                            <Value>SECLAB_WINDOWS_PASSWORD</Value>
                            <PlainText>false</PlainText>
                        </Password>
                        <Group>administrators</Group>
                        <DisplayName>Seclab</DisplayName>
                        <Name>SECLAB_USER</Name>
                        <Description>Seclab User</Description>
                    </LocalAccount>
                </LocalAccounts>
            </UserAccounts>
        </component>
